{
 "manifest_version": 2,
 "name": "Check Jira comment role",
 "permissions": [
    "storage"
  ],
 "icons": { "16": "logo16.png",
           "48": "logo48.png",
          "128": "logo128.png" },
 "options_page": "options.html",
 "description": "Disable comment if you comment public",
 "version": "1",
 "author": "Bartosz Sosna",
 "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-2.1.3.min.js","check.js"]
  }
]
}